{"code":"#include <bits\/stdc++.h>\r\nusing namespace std;\r\n\r\nvoid power(long long T[3][3], long long n)\r\n{\r\n\tif (n == 0 || n == 1)\r\n\t\treturn;\r\n\r\n\tpower(T, n \/ 2);\r\n\r\n\tlong long mod = 1000000007,x1,x2,x3,x4,x5,x6,x7,x8,x9;\r\n\r\n\tlong long a1 = T[0][0], b1 = T[0][1], c1 = T[0][2];\r\n\tlong long d1 = T[1][0], e1 = T[1][1], f1 = T[1][2];\r\n\tlong long g1 = T[2][0], h1 = T[2][1], i1 = T[2][2];\r\n\r\n\tx1 = ((T[0][0] * a1) % mod + (T[0][1] * d1) % mod + (T[0][2] * g1) % mod) % mod;\r\n    x2 = ((T[0][0] * b1) % mod + (T[0][1] * e1) % mod + (T[0][2] * h1) % mod) % mod;\r\n    x3 = ((T[0][0] * c1) % mod + (T[0][1] * f1) % mod + (T[0][2] * i1) % mod) % mod;\r\n    x4 = ((T[1][0] * a1) % mod + (T[1][1] * d1) % mod + (T[1][2] * g1) % mod) % mod;\r\n    x5 = ((T[1][0] * b1) % mod + (T[1][1] * e1) % mod + (T[1][2] * h1) % mod) % mod;\r\n    x6 = ((T[1][0] * c1) % mod + (T[1][1] * f1) % mod + (T[1][2] * i1) % mod) % mod;\r\n    x7 = ((T[2][0] * a1) % mod + (T[2][1] * d1) % mod + (T[2][2] * g1) % mod) % mod;\r\n    x8 = ((T[2][0] * b1) % mod + (T[2][1] * e1) % mod + (T[2][2] * h1) % mod) % mod;\r\n    x9 = ((T[2][0] * c1) % mod + (T[2][1] * f1) % mod + (T[2][2] * i1) % mod) % mod;\r\n    T[0][0] = x1;\r\n    T[0][1] = x2;\r\n    T[0][2] = x3;\r\n    T[1][0] = x4;\r\n    T[1][1] = x5;\r\n    T[1][2] = x6;\r\n    T[2][0] = x7;\r\n    T[2][1] = x8;\r\n    T[2][2] = x9;\r\n\r\n\tif (n % 2) {\r\n        long long a = 1, b = 1, c = 1;\r\n        long long d = 1, e = 0, f = 0;\r\n        long long g = 0, h = 1, i = 0;\r\n\r\n        x1 = ((T[0][0] * a) % mod + (T[0][1] * d) % mod + (T[0][2] * g) % mod) % mod;\r\n        x2 = ((T[0][0] * b) % mod + (T[0][1] * e) % mod + (T[0][2] * h) % mod) % mod;\r\n        x3 = ((T[0][0] * c) % mod + (T[0][1] * f) % mod + (T[0][2] * i) % mod) % mod;\r\n        x4 = ((T[1][0] * a) % mod + (T[1][1] * d) % mod + (T[1][2] * g) % mod) % mod;\r\n        x5 = ((T[1][0] * b) % mod + (T[1][1] * e) % mod + (T[1][2] * h) % mod) % mod;\r\n        x6 = ((T[1][0] * c) % mod + (T[1][1] * f) % mod + (T[1][2] * i) % mod) % mod;\r\n        x7 = ((T[2][0] * a) % mod + (T[2][1] * d) % mod + (T[2][2] * g) % mod) % mod;\r\n        x8 = ((T[2][0] * b) % mod + (T[2][1] * e) % mod + (T[2][2] * h) % mod) % mod;\r\n        x9 = ((T[2][0] * c) % mod + (T[2][1] * f) % mod + (T[2][2] * i) % mod) % mod;\r\n        T[0][0] = x1;\r\n        T[0][1] = x2;\r\n        T[0][2] = x3;\r\n        T[1][0] = x4;\r\n        T[1][1] = x5;\r\n        T[1][2] = x6;\r\n        T[2][0] = x7;\r\n        T[2][1] = x8;\r\n        T[2][2] = x9;\r\n\t}\r\n}\r\nlong long tribonacci(long long n)\r\n{\r\n\tlong long T[3][3] = {{ 1, 1, 1 },\r\n\t\t\t\t{ 1, 0, 0 },\r\n\t\t\t\t{ 0, 1, 0 }};\r\n\r\n\tif (n == 0 || n == 1)\r\n\t\treturn 0;\r\n\telse\r\n\t\tpower(T, n - 2);\r\n\r\n\treturn T[0][0];\r\n}\r\n\r\nint main()\r\n{\r\n\tlong long t;\r\n\tscanf(\"%lld\", &t);\r\n\twhile (t--) {\r\n        long long n;\r\n        scanf(\"%lld\", &n);\r\n        printf(\"%lld\\n\", tribonacci(n + 3));\r\n\t}\r\n\r\n\treturn 0;\r\n}","lang":4}